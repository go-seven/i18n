{
  "name": "go-seven-i18n",
  "version": "0.0.0",
  "private": true,
  "description": "Go Seven internationalization",
  "scripts": {
    "build": "run-p build:*",
    "build:index.ts": "markdown2code --lang typescript Readme.md > dist/index.ts",
    "build:locales": "run-p build:locale:*",
    "build:locale:en": "markdown2code --lang yaml English.md | cson2json > dist/en.json",
    "build:locale:it": "markdown2code --lang yaml Italiano.md | cson2json > dist/it.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/go-seven/i18n.git"
  },
  "keywords": [
    "i18n"
  ],
  "author": "Gianluca Casati (http://g14n.info)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/go-seven/i18n/issues"
  },
  "homepage": "https://go7.li",
  "peerDependencies": {
    "react-intl": "*",
    "tslib": "*"
  },
  "devDependencies": {
    "cson": "^6.9.0",
    "markdown2code": "^1.0.2",
    "npm-run-all": "^4.1.5"
  }
}
